@model SecretViewModel
@{
    ViewData["Title"] = "Secret";
}

<h1>Secret Page</h1>

<table class="table">
    <thead>
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Language</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model.Users)
    {
        <partial model="user" name="_UserData"></partial>          
    }
    </tbody>
</table>

